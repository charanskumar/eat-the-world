<%- include('../partials/nav') -%>
<h1 class="catTitle"><%= category.toUpperCase() %></h1>
<div class="cardContainer">
  <% catFoods.meals.forEach(function(meal) { %>
  <div class="container-fluid">
    <div class="singleCard">
      <div class="catCardStyle">
        <div class="card">
          <a href="/food/meal/<%= meal.idMeal %>">
            <h3 class="catCardTitle"><%= meal.strMeal %></h3>

            <br />
            <img src="<%= meal.strMealThumb %>" alt="Food Picture" />
            <br /><br />
          </a>
        </div>
      </div>
    </div>
  </div>
  <% }); %>
</div>
<%- include('../partials/footer') -%>
