<%- include('../partials/nav') -%>

<h1>Food By Category</h1>
<% categoryList.meals.forEach(function(area){ %>
<div class="container-fluid">
  <div class="singleCard">
    <div class="card">
      <a href="/food/category/<%= area.strCategory %>"
        ><%= area.strCategory %>
        <img
          class="card-img-top"
          src="/public/images/burger.jpg"
          alt="Card image cap"
        />
      </a>
    </div>
  </div>
</div>
<% } ); %> <%- include('../partials/footer') -%>
